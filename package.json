{
    "name": "g-engin",
    "displayName": "G-Engin",
    "description": "Visual Studio Code snippets for <a href=\"https://www.autoitscript.com/wiki/User_Defined_Functions\">UDF</a> <a href=\"https://github.com/G-Engin/g-engin/blob/g-engine/README.md\">G-Engin</a>",
    "icon": "img/GEngin.png",
    "version": "1.0.0",
    "publisher": "RonildoSouza",
    "galleryBanner": {
        "color": "#2fbe70",
        "theme": "light"
    },
    "license": "UNLICENSED",
    "homepage": "https://github.com/G-Engin/GEngin-Snippets/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/G-Engin/GEngin-Snippets"
    },
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Snippets",
        "Other"
    ],
    "keywords": [
        "autoit",
        "g-engin"
    ],
    "activationEvents": [
        "onLanguage:autoit"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "autoit",
                "extensions": [
                    ".au3"
                ]
            }
        ],
        "snippets": [
            {
                "language": "autoit",
                "path": "./snippets/gengin.json"
            }
        ]
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "extensionDependencies": [
        "Damien.autoit"
    ]
}